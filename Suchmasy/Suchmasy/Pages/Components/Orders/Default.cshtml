@using Suchmasy.Models
@model List<Order>


<div class="card-deck margin-20">
    <div class="container">
        <div class="row">
            @foreach (var ord in Model)
            {
                @await Component.InvokeAsync("RequestCard", new { orderCard = ord })
            }
        </div>
    </div>
</div>



