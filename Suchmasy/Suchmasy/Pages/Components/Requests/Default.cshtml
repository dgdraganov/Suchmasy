@using Suchmasy.Models
@model List<Request>


<div class="card-deck margin-20">
    <div class="container">
        <div class="row">
            @foreach (var req in Model)
            {
               
                    @await Component.InvokeAsync("RequestCard", new { requestCard = req })

            }
        </div>
    </div>
</div>



